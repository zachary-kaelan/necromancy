<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <SoundDef>
    <defName>Lich Death</defName>  
    <label>lich-die</label>  
    <description>Wail of a dying lich.</description>  
    <eventNames />  
    <subSounds>
      <li>
        <name>lich-die</name>      
        <onCamera>False</onCamera>      
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Sounds/lich-die</clipPath>
          </li>
        </grains>      
        <volumeRange>
          <min>45</min>        
          <max>55</max>
        </volumeRange>      
        <pitchRange>
          <min>0.85</min>        
          <max>1.15</max>
        </pitchRange>      
        <distRange>
          <min>5</min>        
          <max>100</max>
        </distRange>      
        <startDelayRange>
          <min>0</min>        
          <max>1</max>
        </startDelayRange>      
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

